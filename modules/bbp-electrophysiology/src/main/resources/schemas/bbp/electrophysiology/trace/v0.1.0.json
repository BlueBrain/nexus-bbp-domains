                "{{scheme}}://{{host}}/{{prefix}}/schemas/bbp/core/typedlabeledontologyterm/v1.0.0"],
  "shapes" : [
    {
      "@id" : "this:TraceShape",
      "@type" : "sh:NodeShape",
      "description" : "Electrophysiology trace shape definition. A trace should have an IRI as identifier",
      "targetClass" : "bbpexp:Trace",
      "nodeKind" : "sh:BlankNodeOrIRI",
      "and":[
        {
          "node":"bbpentity:EntityShape"
        },{
          "property" : [ {
            "path": "bbpexp:projectName",
            "name": "Project name",
            "description": "Name of the project that the trace was recorded for",
            "datatype": "xsd:string",
            "maxCount": 1
          }, {
            "path": "bbpexp:sweep",
            "name": "Sweep",
            "description": "Sweep number of trace",
            "datatype": "xsd:integer",
            "minCount": 1,
            "maxCount": 1
          }, {
            "path": "bbpexp:repetition",
            "name": "Repetition",
            "description": "Repetition number of trace",
            "datatype": "xsd:integer",
            "maxCount": 1
          }, {
            "path": "bbpexp:channel",
            "name": "Channel",
            "description": "Channel number of trace",
            "datatype": "xsd:integer",
            "minCount": 1,
            "maxCount": 1
          }, {
            "path": "bbpexp:originalFilename",
            "name": "Original filename",
            "description": "Original filename of binary",
            "datatype": "xsd:string",
            "maxCount": 1
          }, {
            "path": "bbpexp:retrievalDate",
            "name": "Retrieval date",
            "description": "Retrieval date of binary",
            "datatype": "xsd:dateTime",
            "maxCount": 1
          }, {
            "path" : "schema:unitCode",
            "name" : "Data unit",
            "description" : "Unit of measurement of data",
            "node":"bbptlot:UnitCodeOntologyTermShape",
            "maxCount" : 1
          }, {
            "path" : "bbpexp:timeStep",
            "name" : "Time step",
            "description" : "Time step (inverted sampling frequency) of data",
            "node" : "bbpquantValue:QuantitativeValueShape",
            "maxCount" : 1
          },{
            "path": "prov:qualifiedGeneration",
            "name": "Qualified generation",
            "description": "Qualified trace generation",
            "class": "bbpexp:TraceGeneration",
            "minCount": 1,
            "maxCount": 1
          }]
        }
      ]
    }
  ]
