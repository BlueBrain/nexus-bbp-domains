        {
          "node": "{{base}}/schemas/nexus/core/distribution/v0.1.0/shapes/DistributionShape"
        },
        {
          "property": [
            {
              "path": "schema:mediaType",
              "name": "Media type",
              "description": "",
              "in": [
                "application/swc,application/neurolucida,application/neuroml"
              ]
            }
          ]
        }
      ]
    }
  ]
